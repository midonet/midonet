[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group midonet-admins
[access "refs/heads/*"]
	read = group Anonymous Users
	create = group midonet-admin
	exclusiveGroupPermissions = create forgeAuthor forgeCommitter label-Code-Review label-Verified
	label-Code-Review = -2..+2 group midonet
	label-Code-Review = -1..+1 group Anonymous Users
	label-Code-Review = -1..+1 group midonet-bots
	forgeAuthor = group midonet
	label-Verified = -1..+1 group midonet-admin
	label-Verified = -1..+1 group midonet-bots
	forgeCommitter = group midonet
	submit = group midonet
	push = group midonet-admin
	pushMerge = group midonet-admin
[access "refs/tags/*"]
	push = group midonet
	pushTag = group midonet
	pushSignedTag = group midonet
	exclusiveGroupPermissions = push pushTag pushSignedTag
	read = group Anonymous Users
[project]
	description = MidoNet is an open source network virtualisation system for Openstack clouds. Learn more at http://midonet.org
[receive]
	requireSignedOffBy = false
	requireChangeId = true
	createNewChangeForAllNotInTarget = false
[access "refs/drafts/*"]
	push = group Anonymous Users
[access "refs/for/*"]
	read = group Anonymous Users
	push = group Anonymous Users
