[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group midonet-admins
[access "refs/heads/*"]
	read = group Anonymous Users
	exclusiveGroupPermissions = create forgeCommitter push pushMerge label-Code-Review label-Verified labelAs-Verified submit
	label-Code-Review = -2..+2 group midokura-core
	label-Code-Review = -2..+2 group midonet-core
	label-Code-Review = -1..+1 group Anonymous Users
	label-Verified = +1..+1 group midonet
	label-Verified = -1..+1 group midonet-bots
	forgeCommitter = group midonet
	submit = group midonet
	push = +force group midonet
	pushMerge = group midonet
	labelAs-Verified = -1..+1 group midonet
	removeReviewer = group midonet
	create = group midonet
[access "refs/tags/*"]
	pushTag = +force group midonet
	pushSignedTag = group midonet
	exclusiveGroupPermissions = pushTag pushSignedTag
	read = group Anonymous Users
[project]
	description = MidoNet is an open source network virtualisation system for Openstack clouds. Learn more at http://midonet.org
[receive]
	requireSignedOffBy = false
	requireChangeId = true
	createNewChangeForAllNotInTarget = false
[access "refs/drafts/*"]
	push = group Registered Users
	read = group Anonymous Users
[access "refs/publish/*"]
	read = group Anonymous Users
	push = group Registered Users
